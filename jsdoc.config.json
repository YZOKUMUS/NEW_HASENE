{
  "source": {
    "include": ["./js"],
    "includePattern": ".+\\.js$",
    "exclude": ["node_modules", "tests"]
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "template": "node_modules/docdash"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "tags": {
    "allowUnknownTags": true
  }
}







