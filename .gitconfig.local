# Git Configuration for NEW_HASENE Project
# This file ensures correct remote URL and push settings

[remote "origin"]
    url = https://github.com/YZOKUMUS/NEW_HASENE.git
    fetch = +refs/heads/*:refs/remotes/origin/*

[push]
    default = simple
    followTags = true

[pull]
    rebase = false

[branch "main"]
    remote = origin
    merge = refs/heads/main





